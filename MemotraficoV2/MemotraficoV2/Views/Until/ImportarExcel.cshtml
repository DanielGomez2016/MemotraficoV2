
@{
    ViewBag.Title = "Importar Documentos";
}

<div class="" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                @*<h3>Nuevo Usuario</h3>*@
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-offset-1 col-md-10 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Importar Documentos</h2>
                        <div class="clearfix"></div>
                        @* opciones modal cerrar encojer y opciones *@
                        <ul class="nav navbar-right panel_toolbox"></ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="item form-group col-md-12">
                            <label class="control-label col-md-12 col-sm-3 col-xs-12">Importar Centros Educativos</label>
                            <div class="col-md-12 col-sm-6 col-xs-12">
                                <input type="file" id="fileexe" name="fileexe"  class="form-control col-md-12"/>
                            </div>
                        </div>
                        <div class="item form-group col-md-12">
                           <label class="control-label col-md-12 col-sm-3 col-xs-12">Importacion de Municipio y Localidades</label>
                            <div class="col-md-12 col-sm-6 col-xs-12">
                                <input type="file" id="fileexcelm" name="fileexcelm" class="form-control col-md-12" />
                            </div>
                        </div>

                        <div class="ln_solid"></div>
                        <div class="form-group">
                            <div class="col-md-9 col-md-offset-5">
                                @Html.ActionLink("Cancelar", "Index", new object { }, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>