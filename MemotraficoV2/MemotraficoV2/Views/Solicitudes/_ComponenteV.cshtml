@model MemotraficoV2.ViewModels.Validacion_Requerimientos
@{
    Layout = null;
}


<div class="item form-group well col-md-12">
    <table class="table">
        <thead>
            <tr>
                <th class="col-md-4 text-center">Concepto</th>
                <th class="col-md-8 text-center">Descripcion</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td class="text-center">@Html.LabelFor(i => i.ComponenteV.Andadores)</td>
                <td>@Html.TextBoxFor(i => i.ComponenteV.Andadores, new { @class = "form-control col-md-12" })</td>
            </tr>
            <tr>
                <td class="text-center">@Html.LabelFor(i => i.ComponenteV.Rampas)</td>
                <td>@Html.TextBoxFor(i => i.ComponenteV.Rampas, new { @class = "form-control col-md-12" })</td>
            </tr>
            <tr>
                <td class="text-center">@Html.LabelFor(i => i.ComponenteV.Pasamanos)</td>
                <td>@Html.TextBoxFor(i => i.ComponenteV.Pasamanos, new { @class = "form-control col-md-12" })</td>
            </tr>
        </tbody>
    </table>
</div>